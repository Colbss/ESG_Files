<SequenceModel>
	<FSA>
		<state>OkButton</state>
		<state>ChecklistName</state>
		<state>EditChecklist</state>
		<state>SkipButton</state>
		<state>Keyboard</state>
		<state>Initialise</state>
		<state>AddChecklist</state>
		<state>SaveButton</state>
		<state>AddNote</state>
		<state>SelectChecklist</state>
		<startstate>Initialise</startstate>
		<finalstate>SelectChecklist</finalstate>
		<transition>
			<fromstate>Initialise</fromstate>
			<action>Tap</action>
			<tostate>SkipButton</tostate>
		</transition>
		<transition>
			<fromstate>SkipButton</fromstate>
			<action>Tap</action>
			<tostate>AddNote</tostate>
		</transition>
		<transition>
			<fromstate>AddNote</fromstate>
			<action>Tap</action>
			<tostate>AddChecklist</tostate>
		</transition>
		<transition>
			<fromstate>AddChecklist</fromstate>
			<action>Tap</action>
			<tostate>ChecklistName</tostate>
		</transition>
		<transition>
			<fromstate>ChecklistName</fromstate>
			<action>Tap</action>
			<tostate>Keyboard</tostate>
		</transition>
		<transition>
			<fromstate>Keyboard</fromstate>
			<action>Tap</action>
			<tostate>OkButton</tostate>
		</transition>
		<transition>
			<fromstate>OkButton</fromstate>
			<action>Tap</action>
			<tostate>EditChecklist</tostate>
		</transition>
		<transition>
			<fromstate>EditChecklist</fromstate>
			<action>Tap</action>
			<tostate>Keyboard</tostate>
		</transition>
		<transition>
			<fromstate>Keyboard</fromstate>
			<action>Tap</action>
			<tostate>EditChecklist</tostate>
		</transition>
		<transition>
			<fromstate>EditChecklist</fromstate>
			<action>Tap</action>
			<tostate>Keyboard</tostate>
		</transition>
		<transition>
			<fromstate>Keyboard</fromstate>
			<action>Tap</action>
			<tostate>EditChecklist</tostate>
		</transition>
		<transition>
			<fromstate>EditChecklist</fromstate>
			<action>Tap</action>
			<tostate>Keyboard</tostate>
		</transition>
		<transition>
			<fromstate>Keyboard</fromstate>
			<action>Tap</action>
			<tostate>SaveButton</tostate>
		</transition>
		<transition>
			<fromstate>SaveButton</fromstate>
			<action>Observe</action>
			<tostate>SelectChecklist</tostate>
		</transition>
	</FSA>
</SequenceModel>